{
    "user": "SAMP-git",
    "repo": "samp-plugin-timerfix",
    "resources": [
        {
            "name": "^timerfix-(.*)-linux.tar.gz$",
            "platform": "linux",
            "archive": true,
            "includes": ["."],
            "plugins": ["timerfix.so"]
        },
        {
            "name": "^timerfix-(.*)-win32.zip$",
            "platform": "windows",
            "archive": true,
            "includes": ["."],
            "plugins": ["timerfix.dll"]
        }
    ],
    "contributors": ["udan11", "IstuntmanI"],
    "runtime": {
        "plugins": ["SAMP-git/timerfix"]
    }
}